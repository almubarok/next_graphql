{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/api/*": [
        "src/api/*"
      ],
      "@/components/*": [
        "src/components/*"
      ],
      "@/graphql/*": [
        "src/graphql/*"
      ],
      "@/hooks/*": [
        "src/hooks/*"
      ],
      "@/lib/*": [
        "src/lib/*"
      ],
      "@/utils/*": [
        "src/utils/*"
      ],
    },
  },
  "exclude": [
    "node_modules"
  ]
}